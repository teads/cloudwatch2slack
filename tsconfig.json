{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "lib": [
      "es5",
      "es2015.promise"
    ]
  },
  "exclude": [
    "dist",
    "node_modules"
  ]
}
